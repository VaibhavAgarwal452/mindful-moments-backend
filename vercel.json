{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "crons": [
    {
      "path": "/api/v1/users/sendNotificationFromDatabase",
      "schedule": "*/2 * * * *"
    }
  ]
}