{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "crons": [
    {
      "path": "https://mindful-moments-backend.vercel.app/api/v1/users/sendNotificationFromDatabase",
      "schedule": "0 12 * * *"
    }
  ]
}